<core:View
	controllerName="ui.view.pages.dashboard.Dashboard"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:vbm="sap.ui.vbm" >
	<Page
		title="{i18n>/DashboardTitle}"
		showNavButton="true"
		navButtonPress="onBack">
		<subHeader>
			<Bar>
				<contentLeft>
					<SearchField
						search="handleSearch"
						width="100%" >
					</SearchField>
				</contentLeft>
			</Bar>
		</subHeader>

		<Image class="sapUiSmallMargin" width="500px" height="300px" src="{ parts: [ '/address/street', '/address/zip', '/address/city', '/address/country' ], formatter: 'ui.util.Formatter.formatMapUrl' }" />

		<!-- <vbm:GeoMap id="vbi" width="100%" height="100%"/> -->
	</Page>
</core:View>