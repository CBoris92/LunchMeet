<View
	xmlns="sap.m"
	xmlns:layout="sap.ui.layout"
	controllerName="ui.view.pages.Menu">
	<Page
		title="Welcome to Lunchmeet">
		<content>
			<TileContainer>
				<tiles>
					<StandardTile 
						icon="sap-icon://meal"
						press="onToLunchmeetsPage"
						title="Lunchmeets Manager">
					</StandardTile>
					<StandardTile 
						icon="sap-icon://bbyd-dashboard"
						press="onToDashboardPage"
						title="Dashboard">
					</StandardTile>
				</tiles>
			</TileContainer>
		</content>
	</Page>
</View>
