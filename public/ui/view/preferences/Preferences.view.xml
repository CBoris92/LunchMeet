<core:View
	controllerName="ui.view.preferences.Preferences"
	xmlns="sap.uxap"
	xmlns:m="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	height="100%">

	<m:Page
		title="{i18n>/PreferencesTitle}"
		class="sapUiFioriObjectPage"
		showNavButton="{device>/isPhone}"
		navButtonPress="handleNavBackToMenu">

		<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false">
	        <!-- <headerTitle>
	            <ObjectPageHeader headerDesign="Light" objectTitle="{/restaurant/name}" objectSubtitle="{
								path: '/date',
								formatter: 'ui.util.Formatter.date'
							}" objectImageShape="Circle"
	                              objectImageURI="{/restaurant/picture}"
	                              isObjectIconAlwaysVisible="true"
	                              isObjectTitleAlwaysVisible="true">
	            </ObjectPageHeader>
	        </headerTitle> -->

			<!-- <headerContent>
				<l:VerticalLayout>
	        		<m:Label text="{i18n>/RestaurantAddress}" design="Bold"/>
		            <m:Label text="{/restaurant/address}" />
				</l:VerticalLayout>
	        </headerContent> -->

	        <sections>

	            <ObjectPageSection title="{i18n>/TypeCuisine}">
	                <subSections>
	                    <ObjectPageSubSection title=" ">
	                    	<blocks>
	                    		<m:TileContainer
									id="container"
									tileDelete="handleTileDelete"
									tiles="{cuisines>/cuisines}">
									<m:StandardTile
										icon="sap-icon://{icon}"
										title="{cuisines>name}" />
								</m:TileContainer>
								<!-- <m:List items="{cuisines>/cuisines}">
									<m:CustomListItem>
										<m:CheckBox class="myImg" />
									</m:CustomListItem>
								</m:List> -->
							</blocks>
	                    </ObjectPageSubSection>
	                </subSections>
	            </ObjectPageSection>

	            <ObjectPageSection title="{i18n>/TypeRestauration}">
	            	<subSections>
	            		<ObjectPageSubSection title=" ">
	                        <blocks>
	                        	<m:Button/>
	                        </blocks>
	                    </ObjectPageSubSection>
	            	</subSections>
	            </ObjectPageSection>

	            <ObjectPageSection title="{i18n>/Budget}">
	            	<subSections>
	            		<ObjectPageSubSection title=" ">
	                        <blocks>
	                        	<m:Button/>
	                        </blocks>
	                    </ObjectPageSubSection>
	            	</subSections>
	            </ObjectPageSection>

	        </sections>
	    </ObjectPageLayout>

	    <m:footer>
	    	<m:Bar>
	    		<m:contentRight>
	    			<m:Button press="handleEditBtnPress" text="{i18n>/Edit}" icon="{icon>/Edit}" type="Emphasized" />
	    		</m:contentRight>
	    	</m:Bar>
	    </m:footer>

	</m:Page>
</core:View>